fn fibonacci(num: n) -> num {
    if n <= 1 {
    	return n;
    }
    
    num: a = 0;
    num: b = 1;
    num: result = 0;
    num: i = 2;

    while i <= n {
        result = a + b;
        a = b;
        b = result;
        i = i + 1;
    }

    return result;
}

fn main() -> num {
    printf("%i", fibonacci(45));

    return 0;
}
