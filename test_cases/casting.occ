fn test_cast_1() {
    rnum: x = 3.14;
    num: i = x as num; 

    print("%i\n", i);
}

fn test_cast_2() {
    rnum ptr: x = malloc(8);
    deref x = 3.14; // the parser or codegen doesnt handle floats for this?
    
    //num: i = deref x as num ptr; 
    //num: i = deref(long deref)x; 

    print("%i\n", i);
}

fn test_cast_3() {
    rnum ptr: x = malloc(8);
    deref x = 3.14;
    
    num ptr: i = x as num ptr; 

    print("%p\n", i);
}

fn main() {
    test_cast_1(); // 3
    test_cast_2(); // 3
    test_cast_3(); // mem address of i
}